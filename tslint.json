{
    "defaultSeverity": "error",
    "extends": "tslint-config-airbnb",
    "jsRules": {},
    "rules": {
      "semicolon": false,
      "ter-arrow-parens": false,
      "align": false,
      "space-before-function-paren": false,
      "import-name": false,
      "prefer-array-literal" : false,
      "object-shorthand-properties-first" : false,
      "max-line-length" : false,
      "variable-name" : false
    },
    "rulesDirectory": []
  }